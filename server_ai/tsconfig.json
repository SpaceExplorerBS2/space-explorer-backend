{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES2022",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "outDir": "./dist",
    "rootDir": "./src",
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "environment.d.ts"],
  "exclude": ["node_modules", "dist"]
}
